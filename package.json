{
  "name": "mevaker-hamedina-client",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "npm-watch dev-build",
    "dev-build": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint ./app ./shared --ext .ts,.tsx",
    "lint:fix": "eslint . --fix",
    "test": "react-router build --mode test",
    "dev:test": "react-router dev --mode test",
    "dev:mock": "npm-run-all --parallel mock-server start:react-mock",
    "mock-server": "npx tsx mock-server/src/server.ts",
    "start:react-mock": "react-router dev --mode mock"
  },
  "dependencies": {
    "@react-router/node": "7.3.0",
    "@react-router/serve": "7.3.0",
    "@tanstack/react-form": "1.2.1",
    "@tanstack/react-query": "5.68.0",
    "antd": "5.24.3",
    "axios": "1.8.4",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "isbot": "5.1.17",
    "npm-watch": "0.13.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "7.3.0",
    "save-dev": "0.0.1-security"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.22.0",
    "@react-router/dev": "7.3.0",
    "@types/body-parser": "1.19.5",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.1",
    "@types/lodash": "4.17.16",
    "@types/node": "20",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "3.8.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "16.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "react-router-devtools": "1.1.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "watch": {
    "dev-build": {
      "patterns": [
        "app/**/*",
        "shared/**/*"
      ],
      "extensions": [
        "ts",
        "tsx"
      ]
    }
  }
}
